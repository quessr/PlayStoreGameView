<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="GameListView">
        <attr name="cardType" format="enum">
            <enum name="big_image" value="0" />
            <enum name="list" value="1" />
            <enum name="small_image" value="2" />
        </attr>
        <!-- ListCard 타입일 때만 적용되는 spanCount 속성 -->
        <attr name="spanCount" format="integer" />
    </declare-styleable>
</resources>